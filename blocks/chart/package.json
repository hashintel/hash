{
  "name": "@blocks/chart",
  "version": "0.1.1",
  "private": true,
  "description": "A block for plotting entities in a 2D chart",
  "keywords": [
    "blockprotocol",
    "blocks",
    "block"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git#main",
    "directory": "blocks/chart"
  },
  "license": "(MIT OR Apache-2.0)",
  "author": "HASH",
  "scripts": {
    "build": "block-scripts build",
    "codegen": "block-scripts codegen && yarn format",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --fix .",
    "format": "prettier --write --ignore-unknown .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.3.0",
    "@mui/material": "5.14.11",
    "echarts": "^5.3.2",
    "lodash.debounce": "4.0.8",
    "pluralize": "8.0.0",
    "react-hook-form": "7.48.2"
  },
  "devDependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "@types/lodash.debounce": "4.0.7",
    "@types/react-dom": "18.0.9",
    "@types/react-is": "18.2.0",
    "block-scripts": "0.3.1",
    "eslint": "8.33.0",
    "mock-block-dock": "0.1.6",
    "prettier": "2.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "name": "@ben/chart",
    "displayName": "Chart",
    "blockType": {
      "entryPoint": "react"
    },
    "protocol": "0.3",
    "blockEntityType": "https://blockprotocol.org/@hash/types/entity-type/chart/v/1",
    "codegen": {
      "outputFolder": "src/types/generated",
      "targets": {
        "block-entity.ts": [
          {
            "blockEntityType": true
          }
        ]
      }
    },
    "examples": [
      {}
    ],
    "icon": "public/chart-line-regular.svg",
    "image": "public/block-preview.svg"
  }
}

