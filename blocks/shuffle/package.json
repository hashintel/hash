{
  "name": "@blocks/shuffle",
  "version": "0.1.0",
  "description": "Create a list and set it into a random order",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git#main",
    "directory": "blocks/shuffle"
  },
  "license": "MIT",
  "author": "luisbettencourt",
  "scripts": {
    "build": "node_modules/.bin/block-scripts build",
    "dev": "node_modules/.bin/block-scripts dev",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "serve": "node_modules/.bin/block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.18",
    "@dnd-kit/core": "6.0.5",
    "@dnd-kit/sortable": "7.0.1",
    "@dnd-kit/utilities": "3.2.0",
    "@local/hash-design-system": "0.0.0-private",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.6",
    "immer": "9.0.6",
    "lodash.isequal": "4.5.0",
    "typescript": "4.9.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "@types/lodash.isequal": "4.5.6",
    "block-scripts": "0.0.14",
    "eslint": "8.31.0",
    "mock-block-dock": "0.0.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "shuffle",
    "examples": [
      {
        "items": [
          {
            "id": "1",
            "value": "Alice"
          },
          {
            "id": "2",
            "value": "Bob"
          },
          {
            "id": "3",
            "value": "Charlie"
          }
        ]
      }
    ],
    "icon": "public/shuffle.svg",
    "image": "public/preview.png",
    "protocol": "0.2"
  }
}
