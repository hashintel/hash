  "prettier": {
    "printWidth": 80,
    "trailingComma": "all"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.1.0-canary-20230210130343",
    "@fortawesome/free-regular-svg-icons": "6.0.0",
    "@fortawesome/free-solid-svg-icons": "6.0.0",
    "@hashintel/design-system": "0.0.3",
    "@mapbox/search-js-react": "1.0.0-beta.14",
    "@mui/material": "5.11.8",
    "axios": "0.27.2",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "rooks": "7.4.3"
  },
  "devDependencies": {
    "@types/react-dom": "18.0.8",
    "block-scripts": "0.1.0-canary-20230210130343",
    "mock-block-dock": "0.1.0-canary-20230210130343",
    "prettier": "2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "block-scripts": {
    "port": 63212
  },
