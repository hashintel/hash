{
  "name": "@blocks/calculation",
  "version": "0.2.0",
  "description": "Spreadsheet-like table for doing computation on entities",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git",
    "directory": "blocks/calculation"
  },
  "license": "MIT",
  "author": "Ahmad Sattar Atta",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "calculation-block": "0.2.2"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "block-scripts": "0.0.14",
    "eslint": "8.27.0",
    "mock-block-dock": "0.0.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "port": 63210
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Calculation",
    "examples": [
      {
        "cells": [
          [
            [
              "A",
              1
            ],
            "Employees"
          ],
          [
            [
              "B",
              1
            ],
            "Count"
          ],
          [
            [
              "C",
              1
            ],
            "Average"
          ],
          [
            [
              "A",
              2
            ],
            "=count()"
          ],
          [
            [
              "B",
              2
            ],
            "=sum(employees)"
          ],
          [
            [
              "C",
              2
            ],
            "=B2/A2"
          ],
          [
            [
              "A",
              3
            ],
            "=count()"
          ]
        ],
        "rows": [
          [
            1,
            null
          ],
          [
            2,
            "Company"
          ],
          [
            3,
            "Person"
          ]
        ]
      },
      {
        "cells": [],
        "rows": []
      }
    ],
    "icon": "public/sigma.svg",
    "image": "public/block-preview.png",
    "protocol": "0.2"
  }
}
