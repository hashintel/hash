{
  "name": "@blocks/table",
  "version": "0.2.0",
  "private": true,
  "description": "Display and manipulate tabular data with filter, sort, and search features",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git",
    "directory": "blocks/table"
  },
  "license": "MIT",
  "author": "HASH",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.18",
    "@headlessui/react": "1.4.1",
    "lodash": "4.17.21",
    "react-table": "7.7.0",
    "twind": "0.16.17",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "@types/react-table": "7.7.1",
    "block-scripts": "0.0.14",
    "eslint": "8.33.0",
    "mock-block-dock": "0.0.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.9.4",
    "typescript-json-schema": "0.54.0"
  },
  "peerDependencies": {
    "lodash": "4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 62678
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Table",
    "icon": "public/table.svg",
    "image": "public/preview.svg",
    "examples": [
      {
        "initialState": {
          "hiddenColumns": [
            "entityId",
            "name"
          ],
          "columns": [
            {
              "Header": "entityId",
              "accessor": "entityId"
            },
            {
              "Header": "email",
              "accessor": "email"
            },
            {
              "Header": "name",
              "accessor": "name"
            },
            {
              "Header": "age",
              "accessor": "age"
            }
          ]
        }
      }
    ],
    "protocol": "0.2"
  }
}
