    "@local/tsconfig": "0.0.0-private",
    "@types/lodash": "4.17.20",
    "@types/react-dom": "19.1.7",
    "block-scripts": "0.0.14",
    "eslint": "9.20.1",
    "mock-block-dock": "0.0.10",
    "react": "19.1.1",
    "react-dom": "19.1.1",
