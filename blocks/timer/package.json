{
  "name": "@blocks/timer",
  "version": "0.2.0",
  "private": true,
  "description": "Set a countdown timer to remind you when a certain amount of time has elapsed",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "license": "MIT",
  "author": "Tim Diekmann",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.18",
    "date-fns": "2.28.0",
    "duration-fns": "3.0.1"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "block-scripts": "0.0.14",
    "eslint": "8.32.0",
    "mock-block-dock": "0.0.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 63213
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Timer",
    "examples": [
      {
        "initialDuration": "PT5M"
      }
    ],
    "icon": "public/timer.svg",
    "image": "public/block-preview.svg",
    "protocol": "0.2"
  }
}
