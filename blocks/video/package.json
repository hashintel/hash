{
  "name": "@blocks/video",
  "version": "0.2.0",
  "private": true,
  "description": "Embed MP4 and WEBM videos by URL or file upload and add an optional caption",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git#main",
    "directory": "blocks/video"
  },
  "license": "MIT",
  "author": "HASH",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.18",
    "twind": "0.16.17"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "block-scripts": "0.0.14",
    "eslint": "8.33.0",
    "mock-block-dock": "0.0.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 62683
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Video",
    "icon": "public/play-box-outline.svg",
    "image": "public/preview.svg",
    "examples": [
      {
        "initialCaption": "A blooming flower",
        "url": "https://interactive-examples.mdn.mozilla.net/media/cc0-videos/flower.webm"
      }
    ],
    "protocol": "0.2"
  }
}
