{
  "name": "@apps/plugin-browser",
  "version": "0.0.8",
  "private": true,
  "description": "A browser extension for gathering info from the web into your HASH account.",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git"
  },
  "license": "AGPL-3.0",
  "type": "module",
  "workspaces": {
    "nohoist": [
      "@sentry/webpack-plugin"
    ]
  },
  "scripts": {
    "build": "yarn lint:tsc && NODE_ENV=production node utils/build.js",
    "build:firefox": "yarn lint:tsc && BROWSER=firefox NODE_ENV=production node utils/build.js",
    "build:test": "yarn lint:tsc && NODE_ENV=test node utils/build.js",
    "codegen": "rimraf './src/**/*.gen.*'; graphql-codegen --config codegen.config.ts",
    "dev": "NODE_ENV=development node utils/webserver.js",
    "dev:firefox": "BROWSER=firefox node utils/webserver.js",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.4.0-canary.0",
    "@hashintel/block-design-system": "0.0.2",
    "@hashintel/design-system": "0.0.8",
    "@local/advanced-types": "0.0.0-private",
    "@local/hash-graph-sdk": "0.0.0-private",
    "@local/hash-graph-types": "0.0.0-private",
    "@local/hash-isomorphic-utils": "0.0.0-private",
    "@local/hash-subgraph": "0.0.0-private",
    "@mui/material": "5.14.11",
    "@sentry/browser": "7.118.0",
    "bowser": "2.11.0",
    "date-fns": "2.28.0",
    "fractional-indexing": "2.1.0",
    "graphql": "16.8.1",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "9.0.1",
    "webextension-polyfill": "0.10.0",
    "ws": "8.17.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/eslint-parser": "7.23.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "@blockprotocol/core": "0.1.3",
    "@graphql-codegen/cli": "5.0.0",
    "@local/eslint-config": "0.0.0-private",
    "@local/hash-graph-client": "0.0.0-private",
    "@local/hash-isomorphic-utils": "0.0.0-private",
    "@local/hash-subgraph": "0.0.0-private",
    "@local/status": "0.0.0-private",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@sentry/webpack-plugin": "2.21.1",
    "@types/lodash.debounce": "4.0.9",
    "@types/react": "18.2.68",
    "@types/react-dom": "18.2.25",
    "@types/webextension-polyfill": "0.10.4",
    "@types/ws": "8.5.12",
    "babel-loader": "9.1.2",
    "babel-preset-react-app": "10.0.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "dotenv-flow": "3.2.0",
    "file-loader": "6.2.0",
    "fs-extra": "11.1.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "process": "0.11.10",
    "react-refresh": "0.14.0",
    "react-refresh-typescript": "2.0.9",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "source-map-loader": "3.0.2",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.2",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "type-fest": "3.13.1",
    "typescript": "5.5.4",
    "webpack": "5.90.3",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "zip-webpack-plugin": "4.0.1"
  }
}
