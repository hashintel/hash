{
  "name": "@apps/plugin-browser",
  "version": "0.0.9",
  "private": true,
  "description": "A browser extension for gathering info from the web into your HASH account.",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git"
  },
  "license": "AGPL-3.0",
  "type": "module",
  "workspaces": {
    "nohoist": [
      "@sentry/webpack-plugin"
    ]
  },
  "scripts": {
    "build": "yarn lint:tsc && NODE_ENV=production node utils/build.js",
    "build:firefox": "yarn lint:tsc && BROWSER=firefox NODE_ENV=production node utils/build.js",
    "build:test": "yarn lint:tsc && NODE_ENV=test node utils/build.js",
    "codegen": "rimraf './src/**/*.gen.*'; graphql-codegen --config codegen.config.ts",
    "dev": "NODE_ENV=development node utils/webserver.js",
    "dev:firefox": "BROWSER=firefox node utils/webserver.js",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@blockprotocol/graph": "0.4.0-canary.2",
    "@blockprotocol/type-system": "0.1.2-canary.1",
    "@hashintel/block-design-system": "0.0.5",
    "@hashintel/design-system": "0.0.9-canary.2",
    "@local/advanced-types": "0.0.0-private",
    "@local/hash-graph-sdk": "0.0.0-private",
    "@local/hash-isomorphic-utils": "0.0.0-private",
    "@mui/material": "5.18.0",
    "@sentry/browser": "9.44.0",
    "bowser": "2.12.1",
    "date-fns": "4.1.0",
    "fractional-indexing": "3.2.0",
    "graphql": "16.11.0",
    "lodash.debounce": "4.0.8",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uuid": "13.0.0",
    "webextension-polyfill": "0.12.0",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@blockprotocol/core": "0.1.4",
    "@graphql-codegen/cli": "^5.0.3",
    "@local/eslint": "0.0.0-private",
    "@local/hash-graph-client": "0.0.0-private",
    "@local/hash-isomorphic-utils": "0.0.0-private",
    "@local/status": "0.0.0-private",
    "@pmmmwh/react-refresh-webpack-plugin": "0.6.1",
    "@sentry/webpack-plugin": "3.6.1",
    "@types/lodash.debounce": "4.0.9",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/webextension-polyfill": "0.12.4",
    "@types/ws": "8.18.1",
    "babel-loader": "9.2.1",
    "babel-preset-react-app": "10.1.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.11.0",
    "dotenv-flow": "3.3.0",
    "eslint": "9.38.0",
    "file-loader": "6.2.0",
    "fs-extra": "11.3.2",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.6.4",
    "process": "0.11.10",
    "react-refresh": "0.18.0",
    "react-refresh-typescript": "2.0.11",
    "rimraf": "6.0.1",
    "sass": "1.93.2",
    "sass-loader": "13.3.3",
    "source-map-loader": "3.0.2",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.4",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "type-fest": "5.1.0",
    "typescript": "5.9.3",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2",
    "zip-webpack-plugin": "4.0.3"
  }
}
