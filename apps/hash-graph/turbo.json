{
  "extends": ["//"],
  "tasks": {
    "start:migrate": {
      "cache": false,
      "dependsOn": ["compile:release"]
    },
    "start:test:migrate": {
      "cache": false,
      "dependsOn": ["compile"]
    },
    "build:docker": {
      "cache": false
