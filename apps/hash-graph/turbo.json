{
  "extends": ["//"],
  "tasks": {
    "compile": {
      "cache": false
    },
    "compile:release": {
      "cache": false
    },
    "dev:migrate": {
      "dependsOn": ["compile"]
    },
    "dev:server": {
      "dependsOn": ["compile", "dev:migrate"],
      "persistent": true
    },
    "dev:test-server": {
      "dependsOn": ["compile", "dev:migrate"],
      "persistent": true
    },
    "dev:type-fetcher": {
      "dependsOn": ["compile"],
      "persistent": true
    },
    "dev": {
      "dependsOn": ["compile", "dev:migrate"],
      "persistent": true
    },
    "start:migrate": {
      "dependsOn": ["compile:release", "build:release"]
    },
    "start:server": {
      "dependsOn": ["compile:release", "start:migrate"],
      "persistent": true
    },
    "start:type-fetcher": {
      "dependsOn": ["compile:release", "build"],
      "persistent": true
    },
    "start": {
      "dependsOn": ["compile:release", "start:migrate"],
      "persistent": true
    },
    "build:docker": {
      "cache": false
    },
    "build:docker:test": {
      "cache": false
    },
    "build:docker:offline": {
      "cache": false
    },
    "build:docker:prod": {
      "cache": false
    },
    "codegen:generate-openapi-specs": {
      "outputs": ["openapi/openapi.json", "openapi/models/**"]
    },
    "test:integration": {
      "dependsOn": ["build"]
    }
  }
}
