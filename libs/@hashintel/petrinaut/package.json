{
  "name": "@hashintel/petrinaut",
  "version": "0.0.5",
  "description": "A visual editor for Petri nets",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git",
    "directory": "libs/@hashintel/petrinaut"
  },
  "license": "(MIT OR Apache-2.0)",
  "sideEffects": false,
  "type": "module",
  "main": "src/main.ts",
  "types": "src/main.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:site": "vite build --config demo-site/vite.config.ts",
    "dev": "vite --config demo-site/vite.config.ts",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "prepublishOnly": "PACKAGE_DIR=$(pwd) yarn workspace @local/repo-chores exe scripts/prepublish.ts",
    "preview": "vite preview --config demo-site/vite.config.ts",
    "postpublish": "PACKAGE_DIR=$(pwd) yarn workspace @local/repo-chores exe scripts/postpublish.ts",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@ark-ui/react": "5.26.2",
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-typescript": "7.25.9",
    "@babel/standalone": "7.28.5",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@hashintel/ds-components": "0.0.1",
    "@hashintel/ds-helpers": "0.0.1",
    "@mantine/hooks": "8.3.5",
    "@monaco-editor/react": "4.8.0-rc.3",
    "@mui/material": "5.18.0",
    "elkjs": "0.11.0",
    "monaco-editor": "0.55.1",
    "react-icons": "5.5.0",
    "reactflow": "11.11.4",
    "uuid": "13.0.0",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@local/eslint": "0.0.0-private",
    "@pandacss/dev": "1.4.3",
    "@types/babel__standalone": "7.1.9",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.0.4",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.38.0",
    "immer": "10.1.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vite": "7.1.11",
    "vitest": "4.0.13",
    "web-worker": "1.4.1"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
