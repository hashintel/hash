{
  "name": "@hashintel/ds-theme",
  "version": "0.0.1",
  "description": "HASH PandaCSS Preset",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git",
    "directory": "libs/@hashintel/ds-theme"
  },
  "license": "(MIT OR Apache-2.0)",
  "sideEffects": false,
  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "build": "panda codegen && ladle build",
    "codegen": "yarn codegen:tokens && yarn codegen:panda",
    "codegen:panda": "panda codegen --clean",
    "codegen:tokens": "tsx scripts/generate-colors.ts",
    "dev": "panda codegen && ladle serve",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:tsc": "tsc --noEmit",
    "preview": "ladle build && ladle preview"
  },
  "dependencies": {
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@ladle/react": "5.1.1",
    "@pandacss/dev": "1.4.3",
    "@total-typescript/tsconfig": "1.0.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "case-anything": "3.1.0",
    "npm-run-all": "4.1.5",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
