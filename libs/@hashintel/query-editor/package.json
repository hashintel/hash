    "clsx": "1.2.1"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private"
  },
  "peerDependencies": {
    "@mui/material": "^5.11.0",
    "@mui/system": "^5.11.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.37.0"
