  "devDependencies": {
    "@local/eslint": "0.0.0-private",
    "@mui/material": "5.17.1",
    "@mui/system": "5.17.1",
    "eslint": "9.20.1",
    "eslint-plugin-storybook": "0.11.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.58.0",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "@mui/material": "^5.14.0",
    "@mui/system": "^5.14.0",
    "react": "^19.0.0",
