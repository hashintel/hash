{
  "name": "@hashintel/query-editor",
  "version": "0.0.3",
  "description": "A visual editor for Block Protocol entity queries",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git",
    "directory": "libs/@hashintel/query-editor"
  },
  "license": "AGPL-3.0",
  "sideEffects": false,
  "type": "module",
  "main": "src/main.ts",
  "types": "src/main.ts",
  "scripts": {
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "prepublishOnly": "PACKAGE_DIR=$(pwd) yarn workspace @local/repo-chores exe scripts/prepublish.ts",
    "postpublish": "PACKAGE_DIR=$(pwd) yarn workspace @local/repo-chores exe scripts/postpublish.ts"
  },
  "dependencies": {
    "@blockprotocol/graph": "workspace:^",
    "@blockprotocol/type-system": "workspace:^",
    "@fortawesome/free-regular-svg-icons": "6.7.1",
    "@fortawesome/free-solid-svg-icons": "6.7.1",
    "@hashintel/design-system": "workspace:^",
    "clsx": "1.2.1"
  },
  "devDependencies": {
    "@local/eslint-config": "workspace:^",
    "@mui/material": "5.16.7",
    "@mui/system": "5.16.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.53.2",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@mui/material": "^5.14.0",
    "@mui/system": "^5.14.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "7.53.2"
  }
}
