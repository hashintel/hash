{
  "name": "@blockprotocol/graph",
  "version": "0.4.0-canary.0",
  "private": true,
  "description": "Implementation of the Block Protocol Graph service specification for blocks and embedding applications",
  "keywords": [
    "blockprotocol",
    "blocks",
    "graph"
  ],
  "homepage": "https://blockprotocol.org",
  "bugs": {
    "url": "https://github.com/blockprotocol/blockprotocol/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "libs/@blockprotocol/graph"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/cjs/main.d.ts",
      "require": "./dist/cjs/main.cjs",
      "import": "./dist/es/main.js"
    },
    "./codegen": {
      "types": "./dist/cjs/codegen.d.ts",
      "require": "./dist/cjs/codegen.cjs",
      "import": "./dist/es/codegen.js"
    },
    "./custom-element": {
      "types": "./dist/cjs/custom-element.d.ts",
      "require": "./dist/cjs/custom-element.cjs",
      "import": "./dist/es/custom-element.js"
    },
    "./graph-module-json": {
      "types": "./dist/cjs/graph-module-json.d.ts",
      "require": "./dist/cjs/graph-module-json.cjs",
      "import": "./dist/es/graph-module-json.js"
    },
    "./internal": {
      "types": "./dist/cjs/internal.d.ts",
      "require": "./dist/cjs/internal.cjs",
      "import": "./dist/es/internal.js"
    },
    "./react": {
      "types": "./dist/cjs/react.d.ts",
      "require": "./dist/cjs/react.cjs",
      "import": "./dist/es/react.js"
    },
    "./stdlib": {
      "types": "./dist/cjs/stdlib.d.ts",
      "require": "./dist/cjs/stdlib.cjs",
      "import": "./dist/es/stdlib.js"
    }
  },
  "module": "./dist/es/main.js",
  "types": "./dist/cjs/main.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/cjs/main.d.ts"
      ],
      "codegen": [
        "./dist/cjs/codegen.d.ts"
      ],
      "custom-element": [
        "./dist/cjs/custom-element.d.ts"
      ],
      "graph-module-json": [
        "./dist/cjs/graph-module-json.d.ts"
      ],
      "internal": [
        "./dist/cjs/internal.d.ts"
      ],
      "react": [
        "./dist/cjs/react.d.ts"
      ],
      "stdlib": [
        "./dist/cjs/stdlib.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:bundle",
    "build:bundle": "node --max-old-space-size=8192 ../../../node_modules/rollup/dist/bin/rollup -c --bundleConfigAsCjs",
    "clean": "rimraf ./dist/",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@blockprotocol/core": "0.1.3",
    "@blockprotocol/type-system": "0.1.2-canary.0",
    "ajv": "^8.12.0",
    "ajv-formats": "3.0.1",
    "json-schema-to-typescript": "^11.0.2",
    "lit": "^2.4.1",
    "lodash.isequal": "4.5.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/lodash.isequal": "4.5.8",
    "@types/node": "20.12.3",
    "@types/react": "18.2.68",
    "eslint": "8.57.0",
    "rimraf": "5.0.7",
    "rollup": "4.12.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "engines": {
    "node": ">= v20"
  }
}
