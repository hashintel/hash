════ Initial MIR ═══════════════════════════════════════════════════════════════

*thunk {thunk#1}() -> String {
    let %0: String
    let %1: String

    bb0(): {
        switchInt(1) -> [0: bb5(), 1: bb1()]
    }

    bb1(): {
        switchInt(0) -> [0: bb3(), 1: bb2()]
    }

    bb2(): {
        goto -> bb4("inner-then")
    }

    bb3(): {
        goto -> bb4("inner-else")
    }

    bb4(%1): {
        goto -> bb6(%1)
    }

    bb5(): {
        goto -> bb6("outer-else")
    }

    bb6(%0): {
        return %0
    }
}
