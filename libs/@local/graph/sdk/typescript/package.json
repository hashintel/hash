{
  "name": "@local/hash-graph-sdk",
  "version": "0.0.0-private",
  "private": true,
  "description": "SDK for the HASH Graph.",
  "license": "AGPL-3.0",
  "type": "module",
  "exports": {
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.build.json",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "test:unit": "vitest --run"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.4.0-canary.0",
    "@local/advanced-types": "0.0.0-private",
    "@local/harpc-client": "0.0.0-private",
    "@local/hash-graph-client": "0.0.0-private",
    "@local/hash-graph-types": "0.0.0-private",
    "effect": "3.12.3"
  },
  "devDependencies": {
    "@local/eslint": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "@vitest/coverage-istanbul": "2.1.8",
    "eslint": "9.17.0",
    "rimraf": "6.0.1",
    "typescript": "5.7.3",
    "vitest": "2.1.8"
  }
}
