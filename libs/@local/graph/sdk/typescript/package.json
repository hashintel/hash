{
  "name": "@local/hash-graph-sdk",
  "version": "0.0.0-private",
  "private": true,
  "description": "SDK for the HASH Graph.",
  "license": "AGPL-3.0",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.build.json",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "test:unit": "vitest --run"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.4.1",
    "@blockprotocol/type-system": "workspace:*",
    "@local/advanced-types": "workspace:*",
    "@local/harpc-client": "workspace:*",
    "@local/hash-graph-client": "workspace:*",
    "@rust/hash-graph-authorization": "workspace:*",
    "@rust/hash-graph-store": "workspace:*",
    "@temporalio/client": "1.12.1",
    "effect": "3.18.4",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@local/eslint": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "9.39.2",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
