{
  "name": "@local/hash-graph-types",
  "version": "0.0.0-private",
  "private": true,
  "description": "Type definitions for the HASH Graph.",
  "license": "AGPL-3.0",
  "type": "module",
  "exports": {
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.build.json",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@blockprotocol/graph": "workspace:^",
    "@blockprotocol/type-system": "workspace:^",
    "@local/advanced-types": "workspace:^",
    "@local/hash-graph-client": "workspace:^"
  },
  "devDependencies": {
    "@local/eslint-config": "workspace:^",
    "@local/tsconfig": "workspace:^",
    "eslint": "8.57.0",
    "rimraf": "6.0.1",
    "typescript": "5.6.3"
  }
}
