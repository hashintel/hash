  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "eslint": "8.57.0",
    "eslint-plugin-file-extension-in-import-ts": "2.1.0",
    "rimraf": "5.0.7",
    "typescript": "5.5.2"
  }
