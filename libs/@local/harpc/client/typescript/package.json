{
  "name": "@local/harpc-client",
  "version": "0.0.0-private",
  "private": true,
  "description": "Client for the HaRPC system",
  "license": "AGPL-3",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./*": {
      "import": "./dist/src/*/index.js",
      "types": "./dist/src/*/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.build.json",
    "fix:eslint": "eslint --fix .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "test:unit": "vitest --run"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "17.0.0",
    "@chainsafe/libp2p-yamux": "8.0.1",
    "@libp2p/crypto": "5.1.12",
    "@libp2p/identify": "4.0.5",
    "@libp2p/interface": "3.0.2",
    "@libp2p/ping": "3.0.5",
    "@libp2p/tcp": "11.0.5",
    "@multiformats/dns": "1.0.10",
    "@multiformats/multiaddr": "13.0.1",
    "effect": "3.18.4",
    "it-stream-types": "2.0.2",
    "libp2p": "3.0.6",
    "multiformats": "13.4.1",
    "uint8arraylist": "2.4.8"
  },
  "devDependencies": {
    "@effect/cluster": "0.50.6",
    "@effect/experimental": "0.56.0",
    "@effect/platform": "0.92.1",
    "@effect/platform-node": "0.98.4",
    "@effect/rpc": "0.71.1",
    "@effect/sql": "0.46.0",
    "@effect/vitest": "0.26.0",
    "@local/eslint": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "@rust/harpc-wire-protocol": "0.0.0-private",
    "@types/node": "22.18.11",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "9.38.0",
    "rimraf": "6.0.1",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
