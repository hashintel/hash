{
  "name": "@local/effect-dns-hickory",
  "version": "0.0.0-private",
  "private": true,
  "description": "Hickory DNS implementation of DNS effect library",
  "license": "MIT OR Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "napi build dist --platform --release",
    "build:debug": "napi build dist --platform",
    "regenerate-packages": "napi create-npm-dir --target=."
  },
  "devDependencies": {
    "@local/tsconfig": "0.0.0-private",
    "@napi-rs/cli": "2.18.4",
    "vitest": "2.1.9"
  },
  "napi": {
    "name": "effect-dns-hickory",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  }
}
