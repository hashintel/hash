{
  "name": "@hashintel/block-countdown",
  "version": "0.2.0",
  "description": "Countdown to a time and date",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git",
    "directory": "packages/blocks/countdown"
  },
  "license": "MIT",
  "author": "Tim Diekmann",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --ext .ts,.tsx --fix ./src/",
    "lint:eslint": "eslint --ext .ts,.tsx ./src/",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.11-canary.24",
    "date-fns": "2.28.0",
    "react-datepicker": "4.7.0"
  },
  "devDependencies": {
    "@types/react-datepicker": "^4.4.1",
    "block-scripts": "0.0.14",
    "mock-block-dock": "0.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 63215
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Countdown",
    "examples": [
      {
        "title": "The Beta Launch",
        "targetDate": null,
        "displayTime": false
      }
    ],
    "icon": "public/icon.svg",
    "image": "public/preview.svg",
    "protocol": "0.2"
  }
}
