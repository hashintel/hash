{
  "name": "@hashintel/block-video",
  "version": "0.2.0",
  "private": true,
  "description": "Embed MP4 and WEBM videos by URL or file upload and add an optional caption",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git#main",
    "directory": "packages/blocks/video"
  },
  "license": "MIT",
  "author": "HASH",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --ext .ts,.tsx --fix ./src/",
    "lint:eslint": "eslint --ext .ts,.tsx ./src/",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.8"
  },
  "devDependencies": {
    "block-scripts": "0.0.12",
    "eslint": "8.13.0",
    "mock-block-dock": "0.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twind": "0.16.17",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twind": "^0.16.16"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 62683
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Video",
    "icon": "public/play-box-outline.svg",
    "image": "public/preview.svg",
    "examples": [
      {
        "initialCaption": "A blooming flower",
        "url": "https://interactive-examples.mdn.mozilla.net/media/cc0-videos/flower.webm"
      }
    ],
    "protocol": "0.2"
  }
}
