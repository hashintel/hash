{
  "name": "@hashintel/block-timer",
  "version": "0.2.0",
  "description": "Set a countdown timer to remind you when a certain amount of time has elapsed",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "license": "MIT",
  "author": "Tim Diekmann",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --ext .ts,.tsx --fix ./src/",
    "lint:eslint": "eslint --ext .ts,.tsx ./src/",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.8",
    "date-fns": "2.28.0",
    "duration-fns": "3.0.1"
  },
  "devDependencies": {
    "block-scripts": "0.0.13",
    "mock-block-dock": "0.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 63213
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Timer",
    "examples": [
      {
        "initialDuration": "PT5M"
      }
    ],
    "icon": "public/timer.svg",
    "image": "public/block-preview.svg",
    "protocol": "0.2"
  }
}
