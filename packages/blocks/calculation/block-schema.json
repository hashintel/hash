{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "properties": {
    "active": { "$ref": "#/$defs/position" },
    "cells": {
      "type": "array",
      "prefixItems": [{ "$ref": "#/$defs/position" }, { "type": "string" }],
      "minitems": 2,
      "maxitems": 2
    },
    "rows": {
      "type": "array",
      "prefixItems": [{ "type": "string" }, { "type": ["string", "null"] }],
      "minitems": 2,
      "maxitems": 2
    }
  },
  "required": ["cells", "rows"],
  "$defs": {
    "position": {
      "type": "array",
      "prefixItems": [{ "type": "string" }, { "type": "number" }],
      "minitems": 2,
      "maxitems": 2
    }
  }
}
