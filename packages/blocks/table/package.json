{
  "name": "@hashintel/block-table",
  "version": "0.1.0",
  "private": true,
  "description": "Create a table for storing information in rows and columns",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git#main",
    "directory": "packages/blocks/table"
  },
  "license": "MIT",
  "author": "HASH",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --ext .ts,.tsx --fix ./src/",
    "lint:eslint": "eslint --ext .ts,.tsx ./src/",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "blockprotocol": "0.0.7",
    "immer": "9.0.6",
    "lodash": "4.17.21",
    "react-table": "7.7.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/react-table": "7.7.1",
    "block-scripts": "0.0.5",
    "eslint": "8.13.0",
    "mock-block-dock": "0.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "twind": "0.16.17",
    "typescript": "4.6.3",
    "typescript-json-schema": "0.50.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twind": "^0.16.16"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 62678
  },
  "blockprotocol": {
    "displayName": "Table",
    "icon": "public/table.svg",
    "image": "public/preview.svg",
    "examples": [
      {
        "data": {
          "__linkedData": {
            "aggregate": {
              "itemsPerPage": 5,
              "entityTypeId": "d399c523-e152-49ed-81a4-56d9e17196bb",
              "pageNumber": 1,
              "pageCount": 1
            },
            "entityTypeId": "d399c523-e152-49ed-81a4-56d9e17196bb"
          },
          "data": [
            {
              "entityId": "1",
              "name": "Alice",
              "email": "alice@example.com",
              "age": 42,
              "country": "England"
            },
            {
              "entityId": "2",
              "name": "Bob",
              "email": "bob@example.com",
              "age": 34,
              "country": "France"
            },
            {
              "entityId": "3",
              "name": "David",
              "email": "david@example.com",
              "age": 24,
              "country": "Denmark"
            },
            {
              "entityId": "4",
              "name": "Eric",
              "email": "eric@example.com",
              "age": 59,
              "country": "Iceland"
            }
          ]
        },
        "initialState": {
          "columns": [
            {
              "Header": "entityId",
              "accessor": "entityId"
            },
            {
              "Header": "email",
              "accessor": "email"
            },
            {
              "Header": "name",
              "accessor": "name"
            },
            {
              "Header": "age",
              "accessor": "age"
            },
            {
              "Header": "country",
              "accessor": "country"
            }
          ]
        }
      }
    ],
    "protocol": "0.1"
  }
}
