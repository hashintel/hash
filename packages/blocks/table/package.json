{
  "name": "@hashintel/block-table",
  "version": "0.2.0",
  "private": true,
  "description": "Display and manipulate tabular data with filter, sort, and search features",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git",
    "directory": "packages/blocks/table"
  },
  "license": "MIT",
  "author": "HASH",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --ext .ts,.tsx --fix ./src/",
    "lint:eslint": "eslint --ext .ts,.tsx ./src/",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.8",
    "@headlessui/react": "1.4.1",
    "immer": "9.0.6",
    "lodash": "4.17.21",
    "react-table": "7.7.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/react-table": "7.7.1",
    "block-scripts": "0.0.10",
    "eslint": "8.13.0",
    "mock-block-dock": "0.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twind": "0.16.17",
    "typescript": "4.6.3",
    "typescript-json-schema": "0.50.1"
  },
  "peerDependencies": {
    "lodash": "4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twind": "^0.16.16"
  },
  "block-scripts": {
    "devPort": 9090,
    "servePort": 62678
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Table",
    "icon": "public/table.svg",
    "image": "public/preview.svg",
    "examples": [
      {
        "initialState": {
          "hiddenColumns": [
            "entityId",
            "name"
          ],
          "columns": [
            {
              "Header": "entityId",
              "accessor": "entityId"
            },
            {
              "Header": "email",
              "accessor": "email"
            },
            {
              "Header": "name",
              "accessor": "name"
            },
            {
              "Header": "age",
              "accessor": "age"
            }
          ]
        }
      }
    ],
    "protocol": "0.2"
  }
}
