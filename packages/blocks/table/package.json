{
  "name": "@hashintel/block-table",
  "version": "0.1.0",
  "private": true,
  "description": "Create a table for storing information in rows and columns",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git#main",
    "directory": "packages/blocks/table"
  },
  "license": "MIT",
  "author": "HASH",
  "main": "src/index.ts",
  "browser": "dist/main.js",
  "scripts": {
    "build": "yarn clean; cross-env NODE_ENV=production webpack --mode production && yarn schema",
    "build:dev": "yarn clean; cross-env NODE_ENV=development webpack --mode development",
    "clean": "rimraf ./dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --port 9090 --config webpack-dev-server.config.js --open",
    "fix:eslint": "eslint --ext .ts,.tsx --fix ./src/",
    "lint:eslint": "eslint --ext .ts,.tsx ./src/",
    "lint:tsc": "tsc --noEmit",
    "schema": "typescript-json-schema tsconfig.json AppProps --required true --out dist/block-schema.json",
    "serve": "serve -C -l 62678 ./dist/",
    "start": "concurrently -n webpack,webpack-dev-server,schema -c green,cyan,orange \"yarn build:dev -- --watch --verbose\" \"yarn dev\" \"yarn schema\""
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "blockprotocol": "0.0.7",
    "immer": "9.0.6",
    "lodash": "4.17.21",
    "react-table": "7.7.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "^7.12.5",
    "@types/react-dom": "17.0.8",
    "@types/react-table": "7.7.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "babel-loader": "^8.2.2",
    "concurrently": "7.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.12",
    "cross-env": "7.0.3",
    "css-loader": "5.2.6",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "html-webpack-plugin-v4": "npm:html-webpack-plugin@4.5.1",
    "mock-block-dock": "0.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass": "1.35.1",
    "sass-loader": "^10",
    "serve": "13.0.2",
    "style-loader": "2.0.0",
    "twind": "0.16.16",
    "typescript": "4.6.3",
    "typescript-json-schema": "0.50.1",
    "webpack": "^4.44.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twind": "^0.16.16"
  },
  "blockprotocol": {
    "displayName": "Table",
    "icon": "public/table.svg",
    "image": "public/preview.svg",
    "examples": [
      {
        "data": {
          "__linkedData": {
            "aggregate": {
              "itemsPerPage": 5,
              "entityTypeId": "d399c523-e152-49ed-81a4-56d9e17196bb",
              "pageNumber": 1,
              "pageCount": 1
            },
            "entityTypeId": "d399c523-e152-49ed-81a4-56d9e17196bb"
          },
          "data": [
            {
              "entityId": "1",
              "name": "Alice",
              "email": "alice@example.com",
              "age": 42,
              "country": "England"
            },
            {
              "entityId": "2",
              "name": "Bob",
              "email": "bob@example.com",
              "age": 34,
              "country": "France"
            },
            {
              "entityId": "3",
              "name": "David",
              "email": "david@example.com",
              "age": 24,
              "country": "Denmark"
            },
            {
              "entityId": "4",
              "name": "Eric",
              "email": "eric@example.com",
              "age": 59,
              "country": "Iceland"
            }
          ]
        },
        "initialState": {
          "columns": [
            {
              "Header": "entityId",
              "accessor": "entityId"
            },
            {
              "Header": "email",
              "accessor": "email"
            },
            {
              "Header": "name",
              "accessor": "name"
            },
            {
              "Header": "age",
              "accessor": "age"
            },
            {
              "Header": "country",
              "accessor": "country"
            }
          ]
        }
      }
    ],
    "protocol": "0.1"
  }
}
