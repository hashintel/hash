{
  "name": "@hashintel/block-shuffle",
  "version": "0.0.0",
  "description": "Create a list and set it into a random order",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hashintel/hash.git#main",
    "directory": "packages/blocks/shuffle"
  },
  "license": "MIT",
  "author": "luisbettencourt",
  "scripts": {
    "build": "node_modules/.bin/block-scripts build",
    "dev": "node_modules/.bin/block-scripts dev",
    "fix:eslint": "eslint --ext .ts,.tsx --fix ./src/",
    "lint:eslint": "eslint --ext .ts,.tsx ./src/",
    "lint:tsc": "tsc --noEmit",
    "serve": "node_modules/.bin/block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.12",
    "@dnd-kit/core": "6.0.5",
    "@dnd-kit/modifiers": "6.0.0",
    "@dnd-kit/sortable": "7.0.1",
    "@dnd-kit/utilities": "3.2.0",
    "@hashintel/hash-design-system": "0.0.0",
    "@mui/icons-material": "5.4.2",
    "@mui/material": "5.4.3",
    "immer": "9.0.6",
    "lodash.isequal": "4.5.0",
    "typescript": "4.7.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/lodash.isequal": "4.5.6",
    "@types/react-beautiful-dnd": "13.1.2",
    "block-scripts": "0.0.14",
    "mock-block-dock": "0.0.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "shuffle",
    "examples": [
      {
        "items": [
          "Alice",
          "Bob",
          "Charlie"
        ]
      }
    ],
    "icon": "public/shuffle.svg",
    "image": "public/preview.png",
    "protocol": "0.2"
  }
}
