{
  "name": "@hashintel/hash-graph-migrations",
  "version": "0.0.0",
  "private": true,
  "description": "The data store for HASH",
  "license": "AGPL-3.0",
  "scripts": {
    "fix:eslint": "eslint --ext .ts --fix .",
    "graph:generate-ids": "ts-node postgres/scripts/generate-ids.ts",
    "graph:migrate": "ts-node node_modules/.bin/node-pg-migrate -d HASH_GRAPH_PG_MIGRATION_URL --envPath ../.env -f ./postgres/migration-config.json",
    "graph:recreate-db": "ts-node postgres/scripts/recreate-db.ts",
    "lint:eslint": "eslint --ext .ts .",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv-flow": "3.2.0",
    "id128": "1.6.6",
    "node-pg-migrate": "6.2.1",
    "node-postgres": "0.6.2",
    "pg": "8.7.1",
    "prompts": "2.4.1",
    "ts-node": "10.4.0",
    "yargs": "17.1.1"
  },
  "devDependencies": {
    "@types/node-pg-migrate": "2.3.1",
    "@types/prompts": "2.0.14",
  }
}
