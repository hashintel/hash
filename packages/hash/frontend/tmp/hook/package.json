  "keywords": [
    "blockprotocol",
    "blocks",
    "hook"
  ],
  "homepage": "https://blockprotocol.org",
  "bugs": {
    "url": "https://github.com/blockprotocol/blockprotocol/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "packages/@blockprotocol/graph"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
