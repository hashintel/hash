{
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "jsx-a11y/label-has-associated-control": "off",
    "import/no-default-export": "error"
  },
  "overrides": [
    {
      "files": [
        "./src/pages/**/*.page.ts",
        "./src/pages/**/*.page.tsx",
        "**/__mocks__/**"
      ],
      "rules": {
        "import/no-default-export": "off"
      }
    }
  ]
}
