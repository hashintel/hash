{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "target": "esnext",
    "noEmit": true
  },
  "include": [
    "assets.d.ts",
    "next-env.d.ts",
    "theme-override.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": ["node_modules", ".next"]
}
