{
  "name": "@hashintel/block-embed",
  "version": "0.1.0",
  "private": true,
  "description": "Embed external content",
  "license": "MIT",
  "author": "hashintel",
  "main": "src/index.ts",
  "browser": "dist/main.js",
  "scripts": {
    "build": "yarn clean; cross-env NODE_ENV=production webpack --mode production && yarn schema",
    "build:dev": "yarn clean; cross-env NODE_ENV=development webpack --mode development",
    "clean": "rimraf ./dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --port 3000 --config webpack-dev-server.config.js --open",
    "fix": "eslint --ext .ts,.tsx --fix ./src/",
    "lint": "eslint --ext .ts,.tsx ./src/ && tsc --noEmit # @hashintel/block-embed",
    "schema": "typescript-json-schema tsconfig.json AppProps --required true --out dist/block-schema.json",
    "serve": "serve -C -l 62680 ./dist/",
    "start": "concurrently -n webpack,webpack-dev-server -c green,cyan \"yarn build:dev --watch --verbose\" \"yarn dev\""
  },
  "dependencies": {
    "@hashintel/block-protocol": "*",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@apollo/client": "3.3.21",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "^7.12.5",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "babel-loader": "^8.2.2",
    "concurrently": "6.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.12",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "24.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-restricted-imports": "0.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql": "15.5.1",
    "html-webpack-plugin": "^4.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "serve": "12.0.1",
    "twind": "0.16.16",
    "typescript": "4.3.5",
    "typescript-json-schema": "0.50.1",
    "webpack": "^4.44.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "twind": "0.16.16"
  },
  "blockprotocol": {
    "displayName": "Embed",
    "icon": "public/embed.svg"
  }
}
